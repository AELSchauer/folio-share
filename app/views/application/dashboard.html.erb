<h4 align="center">Admin Dashboard</h4>
<div "container" id="main">
<div class="container" id="allfiles" style="overflow:scroll; height:400px;">
  <div class="row">
    <div class="col s12">
      <h5 align="center">All files</h5>
        <table class="bordered highlight">
          <thead>
            <tr>
              <th>User</th>
              <th>File Name</th>
              <th>File Type</th>
              <th>Delete<th>
            </tr>
          </thead>
              <% @uploads.each do |upload|%>
          <tbody>
            <tr>
              <td> <%= upload.user.username%></td>
              <td> <%=link_to "#{upload.name}", upload_path(upload)%></td>
              <td> <%= upload.content_type%></td>
              <td><%= link_to "delete", upload_path(upload), method: :delete%>
            </tr>
              <%end%>
          </tbody>
        </table>
      </div>
    </div>
  </div>
  <br>
  <br>
  <div class="container" id="allusers" style="overflow:scroll; height:400px;">
    <div class="row">
      <div class="col s12">
        <h5 align="center">All Users</h5>
          <table class="bordered highlight">
            <thead>
              <tr>
                <th>UserName</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Status</th>
                <th>Change Status</th>
              </tr>
            </thead>
              <% @users.each do |user|%>
            <tbody>
              <tr>
                <td><%=user.username%></td>
                <td><%=user.first_name%></td>
                <td><%=user.last_name%></td>
                <td><%=user.email%></td>
                <td><%=user.roles.take.name%></td>
              </tr>
              <%end%>
            </tbody>
          </table>
      </div>
    </div>
  </div>
</div>
